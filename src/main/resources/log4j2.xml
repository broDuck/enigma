<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="3> %date{HH:mm:ss}|%thread|%-5level|%class|%method|%line|==> |%msg!%n" />
        </Console>
    </Appenders>

    <Loggers>
        <Root level="all">
            <AppenderRef ref="console" />
        </Root>
    </Loggers>

</Configuration>
