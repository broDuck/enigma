<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >

<generatorConfiguration>
    <classPathEntry location="./libs/mysql/mysql-connector-java/5.1.31/mysql-connector-java-5.1.31.jar" />

    <context id="MySQLtables" targetRuntime="MyBatis3">

        <!-- <plugin type="org.mybatis.generator.plugins.CachePlugin" />	 -->
        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" />
        <plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin">
            <!-- <property name="useEqualsHashCodeFromRoot" value="true"/> -->
        </plugin>

        <commentGenerator>
            <property name="suppressDate" value="true" />
        </commentGenerator>

        <jdbcConnection
                driverClass="com.mysql.jdbc.Driver"
                connectionURL="jdbc:mysql://13.124.77.168:3306/enigma" userId="broduck"
                password="rhffna12"
        ></jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.broduck.enigma.generate.model" targetProject="./src/main/java">
            <property name="enableSubPackages" value="false" />
            <property name="trimStrings" value="true" />
            <!-- <property name="constructorBased" value="true" /> -->
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="generate-sql" targetProject="./src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.broduck.enigma.generate.dao" targetProject="./src/main/java">
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>

        <table tableName="category" domainObjectName="Category"><property name="useActualColumnNames" value="false" /><generatedKey column="category_sn" sqlStatement="MySql" identity="true" /></table>
        <table tableName="member" domainObjectName="Member"><property name="useActualColumnNames" value="false" /><generatedKey column="member_sn" sqlStatement="MySql" identity="true" /></table>
        <table tableName="vote" domainObjectName="Vote"><property name="useActualColumnNames" value="false" /><generatedKey column="vote_sn" sqlStatement="MySql" identity="true" /></table>
        <table tableName="voteitem" domainObjectName="VoteItem"><property name="useActualColumnNames" value="false" /><generatedKey column="vote_item_sn" sqlStatement="MySql" identity="true" /></table>
        <table tableName="voteitemdetail" domainObjectName="VoteItemDetail"><property name="useActualColumnNames" value="false" /><generatedKey column="vote_item_detail_sn" sqlStatement="MySql" identity="true" /></table>
        <table tableName="votejoinmember" domainObjectName="VoteJoinMember"><property name="useActualColumnNames" value="false" /><generatedKey column="vote_join_sn" sqlStatement="MySql" identity="true" /></table>
        <table tableName="votekind" domainObjectName="VoteKind"><property name="useActualColumnNames" value="false" /><generatedKey column="vote_kind_sn" sqlStatement="MySql" identity="true" /></table>
        <table tableName="votekinddetail" domainObjectName="VoteKindDetail"><property name="useActualColumnNames" value="false" /><generatedKey column="vote_kind_detail_sn" sqlStatement="MySql" identity="true" /></table>
